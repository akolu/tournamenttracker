{
  "name": "tournament-tracker",
  "version": "1.0.0",
  "private": true,
  "main": "dist/TournamentTracker.js",
  "types": "dist/types.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.fs.js",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "postinstall": "dotnet tool restore",
    "start": "dotnet fable watch src -o src/build --run webpack serve",
    "clean": "rimraf dist && rimraf src/build",
    "build": "dotnet fable src -o src/build && webpack && copyfiles -u 1 public/* dist",
    "test": "dotnet fable test -o test/build --run jest"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "roots": [
      "./test/build"
    ],
    "testMatch": [
      "<rootDir>/**/*.Test.js"
    ],
    "coveragePathIgnorePatterns": [
      "/.fable/",
      "/[fF]able.*/",
      "/node_modules/"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.7.1",
    "jest": "^27.5.1",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
